#!/bin/bash
declare -a StringArray=("'12-15-2019'" "'12-20-2019'" "'12-25-2019'" "'12-31-2019'" "'2020-01-05'" "'2020-01-10'" "'2020-01-15'" "'2020-01-20'" "'2020-01-25'" "'2020-01-31'" "'2020-02-05'" "'2020-02-10'" "'2020-02-15'" "'2020-02-20'" "'2020-02-25'" "'2020-02-28'")
for i in ${StringArray[@]}; do
   echo "run $i"
   psql -d penny -f new_transformations.sql -v auction_date="$i"
done
